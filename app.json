{
  "expo": {
    "name": "LiSha - List Sharing",
    "slug": "lisha",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#202020",
    "scheme": "lisha",
    "splash": {
      "image": "./assets/splash_white.png",
      "resizeMode": "contain",
      "backgroundColor": "#111111"
    },
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "dark",
      "bundleIdentifier": "com.redigermany.lisha"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "userInterfaceStyle": "dark",
      "package": "com.redigermany.lisha",
      "googleServicesFile": "./android/app/google-services.json"
    },
    "facebookScheme": "fb1896807707303093",
    "facebookAppId": "1896807707303093",
    "facebookDisplayName": "LiSha",
    "facebookAutoLogAppEventsEnabled": true,
    "facebookAdvertiserIDCollectionEnabled": true,
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "/assets/adaptive-icon.png",
          "color": "#ffffff",
          "mode": "production"
        }
      ]
    ]
  }
}
